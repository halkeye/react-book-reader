{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "amd": true,
        "es6": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 1,
        "quotes": [2, "single"],
        "no-unused-vars": 1,
        "camelcase": 0,
        "no-underscore-dangle": 0
    },
    "1ecmaFeatures": {
      "blockBindings": true
    }

}
